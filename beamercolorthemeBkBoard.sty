% beamercolorthemeBkboard.sty

\definecolor{bkboard}{rgb}{0,0.25,0}
\definecolor{alertedyellow}{rgb}{1,1,0.25}
\definecolor{structuredred}{rgb}{1,0.25,0}

\setbeamercolor{background canvas}{bg=bkboard}
\setbeamercolor{structure}{fg=structuredred}
\setbeamercolor{alerted text}{fg=alertedyellow}

\setbeamercolor{block title}{fg=white, bg=bkboard!60!black}
\setbeamercolor{block body}{fg=white, bg=bkboard!80!black}

\setbeamercovered{transparent=25}
